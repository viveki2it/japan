<%- if false and devise_mapping.registerable? && controller_name != 'registrations' %>
    <%= link_to t(:sign_up), new_registration_path(resource_name) %>&nbsp;
<% end -%>

<%- if devise_mapping.recoverable? && controller_name != 'passwords' %>
    <%= link_to t(:forgot_password), new_password_path(resource_name) %>&nbsp;
<% end -%>

<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
    <%= link_to t(:resend_confirmation_instructions), new_confirmation_path(resource_name) %>&nbsp;
<% end -%>

<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
    <%= link_to "Mail pour débloquer votre compte non reçu ?", new_unlock_path(resource_name) %>&nbsp;
<% end -%>

